{
  "name": "smart-read-ai",
  "version": "0.1.1",
  "description": "智阅AI - 聚焦技术/学术专业阅读场景的 Chrome 浏览器插件，上下文感知的智能阅读助手",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build && node scripts/copy-icons.js",
    "preview": "vite preview",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,css}\"",
    "type-check": "tsc --noEmit",
    "prepare": "husky"
  },
  "keywords": [
    "chrome-extension",
    "browser-extension",
    "typescript",
    "react",
    "vite",
    "ai",
    "reading-assistant",
    "knowledge-graph",
    "gemini-nano",
    "gpt-4",
    "claude",
    "technical-reading",
    "academic-reading"
  ],
  "author": "wenxy",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/chrome": "^0.1.33",
    "@types/node": "^25.0.8",
    "@types/react": "^19.2.8",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.53.0",
    "@typescript-eslint/parser": "^8.53.0",
    "@vitejs/plugin-react": "^5.1.2",
    "chrome-webstore-upload": "^4.0.3",
    "eslint": "^9.39.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.8.0",
    "sharp": "^0.34.5",
    "terser": "^5.46.0",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vite-plugin-web-extension": "^4.5.0"
  },
  "dependencies": {
    "react": "^19.2.3",
    "react-dom": "^19.2.3"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  }
}
